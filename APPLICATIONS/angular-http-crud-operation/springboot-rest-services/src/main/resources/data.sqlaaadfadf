DROP TABLE IF EXISTS employee;

CREATE TABLE user (
  id INT AUTO_INCREMENT  PRIMARY KEY,
  userName VARCHAR(250) NOT NULL,
  userPwd VARCHAR(250) NOT NULL,
  email VARCHAR(250) DEFAULT NULL
);

